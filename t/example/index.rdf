<media:thumbnail url="http://3.bp.blogspot.com/_cgZUdkW7lzE/SAGuyvvplwI/AAAAAAAAARc/0zq6bSmKY7M/s72-c/2332475169_d04a297de9%5B1%5D.jpg" xmlns:media="http://search.yahoo.com/mrss/" width="72" height="72" />
